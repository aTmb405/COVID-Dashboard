{
    "name": "covid-dashboard",
    "version": "0.0.1",
    "description": "An Express, React with Typescript and Less boilerplate",
    "main": "server/index.js",
    "engines": {
        "node": ">=6"
    },
    "proxy": "http://localhost:3001",
    "scripts": {
        "copy-dependencies": "copyfiles -a src/server/data/**/* dist/data -u 3 && copyfiles -a src/server/data/* dist/data -u 3 && copyfiles -a src/server/static/**/* dist/static -u 3",
        "build": "webpack --mode production --config webpack.dev.js && yarn run copy-dependencies",
        "start": "npm run build && tsc -p tsconfig.server.json && npm run server",
        "client": "webpack-dev-server --mode development --devtool inline-source-map --hot --config webpack.dev.js",
        "dev-server": "tsc -p tsconfig.server.json && yarn run copy-dependencies && ts-node dist/",
        "server": "ts-node dist/",
        "dev": "concurrently \"nodemon\" \"npm run client\" -n browser,server -c \"bgBlue.bold,bgMagenta.bold\"",
        "deploy": "webpack --mode production --config webpack.prd.js && tsc -p tsconfig.server.json && yarn run copy-dependencies"
    },
    "author": "Brandon Hafenrichter",
    "license": "MIT",
    "moduleDirectories": [
        "node_modules",
        "src"
    ],
    "dependencies": {
        "@babel/core": "^7.0.0",
        "@babel/plugin-proposal-class-properties": "^7.0.0",
        "@babel/plugin-transform-runtime": "^7.12.10",
        "@babel/preset-env": "^7.0.0",
        "@babel/preset-react": "^7.0.0",
        "@tsconfig/node12": "^1.0.7",
        "@types/babel__core": "^7.1.12",
        "@types/compression": "^1.7.0",
        "@types/express": "^4.17.8",
        "@types/jest": "^26.0.20",
        "@types/memory-cache": "^0.2.1",
        "@types/mongoose": "^5.7.37",
        "@types/node": "^14.11.8",
        "@types/node-fetch": "^2.5.7",
        "@types/react": "^16.8.16",
        "@types/react-dom": "^16.8.4",
        "@types/react-ga": "^2.3.0",
        "@types/react-redux": "^7.1.10",
        "@types/request": "^2.48.5",
        "awesome-typescript-loader": "^5.2.1",
        "babel-eslint": "^10.0.0",
        "babel-loader": "^8.0.0",
        "babel-polyfill": "^6.26.0",
        "body-parser": "^1.19.0",
        "clean-webpack-plugin": "^1.0.0",
        "compression": "^1.7.4",
        "compression-webpack-plugin": "^6.0.5",
        "concurrently": "^4.0.0",
        "copy-webpack-plugin": "^5.0.3",
        "copyfiles": "^2.4.1",
        "core-js": "^3.8.2",
        "css-loader": "^2.0.0",
        "csvtojson": "^2.0.10",
        "eslint": "^5.0.0",
        "eslint-config-airbnb": "^17.0.0",
        "eslint-plugin-import": "^2.11.0",
        "eslint-plugin-jsx-a11y": "^6.0.3",
        "eslint-plugin-react": "^7.7.0",
        "eventemitter3": "^4.0.7",
        "express": "^4.16.3",
        "file-loader": "^3.0.0",
        "html-webpack-plugin": "^3.2.0",
        "i18n-iso-countries": "^6.4.0",
        "jest": "^26.6.3",
        "less": "^3.9.0",
        "less-loader": "^5.0.0",
        "memory-cache": "^0.2.0",
        "mini-css-extract-plugin": "^0.6.0",
        "moment": "^2.29.1",
        "mongoose": "^5.10.11",
        "node": "^15.4.0",
        "node-fetch": "^2.6.1",
        "nodemon": "^1.17.3",
        "react": "^16.5.2",
        "react-bootstrap": "^1.4.3",
        "react-card-flip": "^1.0.11",
        "react-cookie": "^4.0.3",
