        "react-ga": "^3.3.0",
        "react-icons": "^4.1.0",
        "react-lazyload": "^3.1.0",
        "react-redux": "^7.2.2",
        "recharts": "^2.0.2",
        "regenerator-runtime": "^0.13.7",
        "request": "^2.88.2",
        "rodal": "^1.8.1",
        "source-map-loader": "^0.2.4",
        "style-loader": "^0.23.0",
        "svg-url-loader": "^7.1.1",
        "ts-jest": "^26.4.4",
        "ts-node": "^9.1.1",
        "typescript": "^3.4.5",
        "uglifyjs-webpack-plugin": "^2.2.0",
        "url-loader": "^1.0.1",
        "webpack": "^4.5.0",
        "webpack-cli": "^3.0.8",
        "webpack-dev-server": "^3.1.3"
    },
    "devDependencies": {
        "@types/react-lazyload": "^3.0.0"
    }
}
